<p id="notice"><%= notice %></p>

<h1>Calibrado Damage Samples</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @calibrado_damage_samples.each do |calibrado_damage_sample| %>
      <tr>
        <td><%= link_to 'Show', calibrado_damage_sample %></td>
        <td><%= link_to 'Edit', edit_calibrado_damage_sample_path(calibrado_damage_sample) %></td>
        <td><%= link_to 'Destroy', calibrado_damage_sample, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

