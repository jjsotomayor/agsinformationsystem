<br><br><br>
<%= form_for(resource, as: resource_name, url: session_path(resource_name), html: {class: 'form-signin'} )do |f| %>
<%= devise_error_messages! %>
<h3 class="form-signin-heading">Ingreso administrativos</h3>
  <div class="field-sign-in">
    <%= f.label :usuario %><br />
    <%= f.email_field :email, autofocus: true, class:"form-control" %>
  </div>

  <div class="field-sign-in">
    <%= f.label :contraseÃ±a %><br />
    <%= f.password_field :password, autocomplete: "off", class:"form-control" %>
  </div>

  <% if devise_mapping.rememberable? -%>
    <div class="field-sign-in">
      <%= f.check_box :remember_me %>
      <%= f.label :Recuerdame %>
    </div>
  <% end -%>

  <div class="actions">
      <%= f.submit "Ingresar", class: 'btn btn-lg btn-primary btn-block' %>
  </div>
  <%= render "devise/shared/links" %>
<% end %>
