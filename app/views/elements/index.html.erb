<p id="notice"><%= notice %></p>

<h1>Elements</h1>

<table>
  <thead>
    <tr>
      <th>Tag</th>
      <th>Lot</th>
      <th>Process order</th>
      <th>Product type</th>
      <th>Drying method</th>
      <th>Previous usda</th>
      <th>Ex tag</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @elements.each do |element| %>
      <tr>
        <td><%= element.tag %></td>
        <td><%= element.lot %></td>
        <td><%= element.process_order %></td>
        <td><%= element.product_type.name %></td>
        <td><%= element.drying_method.name %></td>
        <td><%= element.previous_usda %></td>
        <td><%= element.ex_tag %></td>
        <td><%= link_to 'Show', element %></td>
        <td><%= link_to 'Edit', edit_element_path(element) %></td>
        <td><%= link_to 'Destroy', element, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Element', new_element_path %>
