<p id="notice"><%= notice %></p>

<h1>Secado Damage Samples</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @secado_damage_samples.each do |secado_damage_sample| %>
      <tr>
        <td><%= link_to 'Show', secado_damage_sample %></td>
        <td><%= link_to 'Edit', edit_secado_damage_sample_path(secado_damage_sample) %></td>
        <td><%= link_to 'Destroy', secado_damage_sample, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

